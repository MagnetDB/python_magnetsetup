{
    "Name": "Fully coupled Axi Transient model",
    "ShortName":"FullAxiTrans",
    "Models":
    {
		"cfpdes":{
			"equations":["heat","magnetic","elastic1","elastic2"]
		},
		"heat":{
			"common":{
                "setup":{
					"unknown":
					{
						"basis":"Pch1",
						"name":"temperature",
						"symbol":"T"
					}
				}
			},
			"models":[
				
			]
		},
		"magnetic":{
			"common":{
                "setup":{
					"unknown":
					{
						"basis":"Pch2",
						"name":"phi",
						"symbol":"phi"
					}
				}
			},
			"models":[
				{
					"name":"magnetic_Air",
					"materials":"Air",
					"setup":{
						"coefficients":{
							"c":"x/mu0:x:mu0",
							"beta":"{2/mu0,0}:mu0"
						}
					}
				}
			]
		},
		"elastic1":{
			"common":{
                "setup":{
					"unknown":
					{
						"basis":"Pchv1",
						"name":"displacement",
						"symbol":"u"
					}
				}
			},
			"models":[
				
			]
		},
		"elastic2":{
			"common":{
                "setup":{
					"unknown":
					{
						"basis":"Pchv1",
						"name":"speed",
						"symbol":"v"
					}
				}
			},
			"models":[
				
			]
		}
    },
    "Parameters":
    {
		{{#Parameters}}
		"{{name}}": "{{value}}",
		{{/Parameters}}
		
		"T0":"293.",
		"Tin":"284.15"
    },
	"Meshes": {
        "cfpdes": {
            "Fields": {
                "U": {
                    "filename": "$cfgdir/U.h5",
                    "basis": "Pch1"
                }
            }
        }
    },
    "Materials":
    {
		"Air":
		{

		}
    },
    "BoundaryConditions":
    {
		"magnetic":
		{
		    "Dirichlet":
	    	{
				"magdir":
				{
		    		"markers":["ZAxis","Infty"],
		    		"expr":"0"
				}
			}
	    },
		"heat":
		{
	    	"Robin":
	    	{
				{{#boundary_Therm_Robin}}
				"{{name}}":
				{
					"expr1": "{{expr1}}",
					"expr2": "{{expr2}}"
				},
				{{/boundary_Therm_Robin}}
			}
		},
		"elastic1":
		{
	    	"Dirichlet":
	    	{
				{{#boundary_Meca_Dir}}
				"{{name}}":
				{
					"expr": "{{value}}"
				},
				{{/boundary_Meca_Dir}}
	    	}
		}
    },
    "InitialConditions":
    {
		"heat":{
			"temperature":
			{
				"Expression":
				{
					{{#init_temp}}
					"{{name}}":
					{
						"markers": {{magnet_parts}},
						"expr":"{{name}}_Tinit:{{name}}_Tinit"
					},
					{{/init_temp}}
				}
			}
		}
    },
    "PostProcess":
    {
		"use-model-name":1,
		"cfpdes":
		{
	    	"Exports":
	    	{
				"fields":["heat.temperature","elastic1.displacement"],
				"expr":
				{
		    		"atheta":"magnetic_phi/x:magnetic_phi:x",
		    		"B":
		    		{
						"expr":"{-magnetic_grad_phi_1/x,magnetic_grad_phi_0/x}:magnetic_grad_phi_0:magnetic_grad_phi_1:x",
						"representation":["element"]
		    		},
		    		"U":
		    		{
						"expr":"materials_Conductor_U:materials_Conductor_U",
						"markers": {{part_electric}}
		    		},
		    		"Jth":
		    		{
						"expr":"-materials_Conductor_sigma*(materials_Conductor_U/(2*pi)+magnetic_dphi_dt)/x:magnetic_dphi_dt:materials_Conductor_sigma:materials_Conductor_U:x",
						"markers": {{part_electric}}
		    		},
		    		"Qth":
		    		{
						"expr":"materials_Conductor_sigma*(materials_Conductor_U/(2*pi)+magnetic_dphi_dt)*(materials_Conductor_U/(2*pi)+magnetic_dphi_dt):magnetic_dphi_dt:materials_Conductor_sigma:materials_Conductor_U:x",
						"markers": {{part_electric}}
		    		},
		    		"Flaplace":
		    		{
						"expr":"{materials_Conductor_F_laplace_0,materials_Conductor_F_laplace_1}:materials_Conductor_F_laplace_0:materials_Conductor_F_laplace_1",
						"markers": {{part_electric}}
		    		},
		    		"stress":
		    		{
						"expr":"{materials_stress_rr,materials_stress_rz, materials_stress_rz,materials_stress_zz}:materials_stress_rr:materials_stress_rz:materials_stress_zz",
						"markers": {{part_thermic}},
						"representation":["element"]
		    		},
		    		"HoopStress":
		    		{
						"expr":"materials_stress_thth:materials_stress_thth",
						"markers": {{part_thermic}},
						"representation":["element"]
		    		},
		    		"Vonmises":
		    		{

						"expr": "sqrt(1/2*((materials_stress1-materials_stress2)*(materials_stress1-materials_stress2)+(materials_stress2-materials_stress3)*(materials_stress2-materials_stress3)+(materials_stress1-materials_stress3)*(materials_stress1-materials_stress3))):materials_stress1:materials_stress2:materials_stress3",
						"markers": {{part_thermic}},

						"representation":["element"]
		    		},
		    		"strain":
		    		{

						"expr":"{elastic1_grad_u_00,1/2*(elastic1_grad_u_01+elastic1_grad_u_10), 1/2*(elastic1_grad_u_01+elastic1_grad_u_10),elastic1_grad_u_11}:elastic1_grad_u_00:elastic1_grad_u_01:elastic1_grad_u_10:elastic1_grad_u_11",
						"markers": {{part_thermic}},
						"representation":["element"]
		    		},
					"HoopStrain":
					{
						"expr": "elastic1_u_0/x:x:elastic1_u_0",
						"markers": {{part_thermic}},
						"representation":["element"]
					}

				}
	    	}
		},
		"magnetic":
		{
	    	"Save":
	    	{
				"Fields":
				{
		    		"names":["phi"]
				}
	    	},
	    	"Measures": 
	    	{
				"Statistics":
				{
		    		"MagneticEnergy":
		    		{
						"type":"integrate",
						"expr":"-2*pi*magnetic_phi/x*materials_Conductor_sigma*(materials_Conductor_U/2/pi+magnetic_dphi_dt):magnetic_dphi_dt:magnetic_phi:materials_Conductor_sigma:materials_Conductor_U:x",
						"markers": {{part_electric}}
				}
                                },
				"Points":
				{
				"B0":
				{
                                    "coord":"{1e-7,0.}",
                                    "fields":"Atheta",
			            "expressions":
                                    {
                                          "Bz": "magnetic_grad_Atheta_0+magnetic_Atheta/x:magnetic_Atheta:magnetic_grad_Atheta_0:magnetic_grad_Atheta_1:x"
                                    }
				}
			}
	    	}
		},
		"heat":
		{
	    	"Save":
	    	{
				"Fields":
				{
		    		"names":["temperature"]
				}
	    	},
	    	"Measures":
	    	{
				"Statistics":
				{
					{{#power_magnet}}
					"Power_{{name}}":
					{
        				"type": "integrate",
        				"expr": "2*pi*materials_Conductor_sigma*(materials_Conductor_U/2/pi+magnetic_dphi_dt)*(materials_Conductor_U/2/pi+magnetic_dphi_dt)/x:magnetic_dphi_dt:materials_Conductor_sigma:materials_Conductor_U:x",
        				"markers": {{magnet_parts}}
					},
					{{/power_magnet}}
		    		"Power":
				    {
        				"type": "integrate",
        				"expr": "2*pi*materials_Conductor_sigma*(materials_Conductor_U/2/pi+magnetic_dphi_dt)*(materials_Conductor_U/2/pi+magnetic_dphi_dt)/x:magnetic_dphi_dt:materials_Conductor_sigma:materials_Conductor_U:x",
        				"markers": {{part_electric}}
    				},
				"Stat_T": 
		    		{
						"type":["min","max","mean"], 
						"field":"temperature" 
		    		}
				}
	    	}
		},
		"elastic1":
		{
	    	"Save":
	    	{
				"Fields":
				{
		    		"names":["displacement"]
				}
	    	}
		},
		"elastic2":
		{
	    	"Save":
	    	{
				"Fields":
				{
		    		"names":["speed"]
				}
	    	}
		}
    }
}
