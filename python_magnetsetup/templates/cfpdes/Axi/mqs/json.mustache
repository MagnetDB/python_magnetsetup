{
    "Name": "Axi Magneto Quasi Static model",
    "ShortName":"AxiMqs",
    "Models":
    {
		"cfpdes":{
			"equations":"magnetic"
		},
		"magnetic":{
			"common":{
                "setup":{
					"unknown":
					{
						"basis":"Pch2",
						"name":"phi",
						"symbol":"phi"
					}
				}
			},
			"models":[
				{
                    "name":"magnetic_conductor",
                    "materials":{{part_conductor}},
                    "setup":{
                        "coefficients":{
							"c":"x/materials_mu:x:materials_mu",
    						"beta":"{2/materials_mu,0}:materials_mu",
                            "f":"materials_j_th*x*x:materials_j_th:x",
    						"d":"materials_sigma*x:x:materials_sigma"
                        }
                    }
                },{
					"name":"magnetic_insulator",
					"materials":{{part_insulator}},
					"setup":{
						"coefficients":{
							"c":"x/materials_mu:x:materials_mu",
    						"beta":"{2/materials_mu,0}:materials_mu"
						}
					}
				},{
					"name":"magnetic_Air",
					"materials":"Air",
					"setup":{
						"coefficients":{
							"c":"x/mu0:x:mu0",
							"beta":"{2/mu0,0}:mu0"
						}
					}
				}
			]
		}
    },
    "Parameters":
    {
		{{#Parameters}}
		"{{name}}": "{{value}}",
		{{/Parameters}}

		"T0":"293."
    },
    "Materials":
    {
		"Air":
		{

		}
    },
    "BoundaryConditions":
    {
		"magnetic":
		{
		    "Dirichlet":
	    	{
				"magdir":
				{
		    		"markers":["ZAxis","Infty"],
		    		"expr":"0"
				}
			}
	    }
    },
    "PostProcess":
    {
		"use-model-name":1,
		"cfpdes":
		{
	    	"Exports":
	    	{
				"expr":
				{
		    		"atheta":"magnetic_phi/x:magnetic_phi:x",
		    		"B":
		    		{
						"expr":"{-magnetic_grad_phi_1/x,magnetic_grad_phi_0/x}:magnetic_grad_phi_0:magnetic_grad_phi_1:x",
						"representation":["element"]
		    		},
					"U":
		    		{
						"expr":"materials_U:materials_U",
						"markers": {{part_electric}}
		    		},
		    		"Jth":
		    		{
						"expr":"-materials_sigma*(materials_U/(2*pi)+magnetic_dphi_dt)/x:magnetic_dphi_dt:materials_sigma:materials_U:x",
						"markers": {{part_electric}}
		    		}
				}
	    	}
		},
		"magnetic":
		{
	    	"Measures": 
	    	{
				"Statistics":
				{
		    		"MagneticEnergy":
		    		{
						"type":"integrate",
						"expr":"-2*pi*magnetic_phi/x*materials_sigma*(materials_U/2/pi+magnetic_dphi_dt):magnetic_phi:magnetic_dphi_dt:materials_sigma:materials_U:x",
						"markers": {{part_electric}}
		    		}
				}
	    	}
		}
    }
}
