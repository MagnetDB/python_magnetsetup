{
    "Name": "Fully coupled Axi Stationnary model",
    "ShortName":"FullAxiStat",
    "Models":
    {
		"cfpdes":{
			"equations":["heat","magnetic","elastic"]
		},
		"heat":{
			"common":{
                "setup":{
					"unknown":
					{
						"basis":"Pch1",
						"name":"temperature",
						"symbol":"T"
					}
				}
			},
			"models":[
				
			]
		},
		"magnetic":{
			"common":{
                "setup":{
					"unknown":
					{
						"basis":"Pch1",
						"name":"Atheta",
						"symbol":"Atheta"
					}
				}
			},
			"models":[
				{
					"name":"magnetic_Air",
					"materials":"Air",
					"setup":{
						"coefficients":{
							"c":"x/mu0:x:mu0",
							"a":"1/mu0/x:x:mu0"
						}
					}
				}
			]
		},
		"elastic":{
			"common":{
                "setup":{
					"unknown":
					{
						"basis":"Pchv1",
						"name":"displacement",
						"symbol":"u"
					}
				}
			},
			"models":[
				
			]
		}
    },
    "Parameters":
    {
		{{#Parameters}}
		"{{name}}": "{{value}}",
		{{/Parameters}}
		
		"T0":"293.",
		"Tin":"284.15"
    },
    "Materials":
    {
		"Air":
		{

		}
    },
	"Meshes": {
        "cfpdes": {
            "Fields": {
                "U": {
                    "filename": "$cfgdir/U.h5",
                    "basis": "Pch1"
                }
            }
        }
    },
    "BoundaryConditions":
    {
		"magnetic":
		{
		    "Dirichlet":
	    	{
				"magdir":
				{
		    		"markers":["ZAxis","Infty"],
		    		"expr":"0"
				}
			}
	    },
		"heat":
		{
	    	"Robin":
	    	{
				{{#boundary_Therm_Robin}}
				"{{name}}":
				{
					"expr1": "{{expr1}}",
					"expr2": "{{expr2}}"
				},
				{{/boundary_Therm_Robin}}
			}
		},
		"elastic":
		{
	    	"Dirichlet":
	    	{
				{{#boundary_Meca_Dir}}
				"{{name}}":
				{
					"expr": "{{value}}"
				},
				{{/boundary_Meca_Dir}}
	    	}
		}
    },
    "InitialConditions":
    {
		"heat":{
			"temperature":
			{
				"Expression":
				{
					{{#init_temp}}
					"{{name}}":
					{
						"markers": {{magnet_parts}},
						"expr":"{{name}}_Tinit:{{name}}_Tinit"
					},
					{{/init_temp}}
				}
			}
		}
    },
    "PostProcess":
    {
		"use-model-name":1,
		"cfpdes":
		{
	    	"Exports":
	    	{
				"fields":["heat.temperature","elastic.displacement"],
				"expr":
				{
		    		"atheta":"magnetic_Atheta:magnetic_Atheta",
		    		"B":
		    		{
						"expr":"{-magnetic_grad_Atheta_1,magnetic_grad_Atheta_0+magnetic_Atheta/x}:magnetic_Atheta:magnetic_grad_Atheta_0:magnetic_grad_Atheta_1:x",
						"representation":["element"]
		    		},
		    		"U":
		    		{
						"expr":"materials_Conductor_U:materials_Conductor_U",
						"markers": {{part_electric}}
		    		},
		    		"Jth":
		    		{
						"expr":"-materials_Conductor_sigma*materials_Conductor_U/(2*pi*x):materials_Conductor_sigma:materials_Conductor_U:x",
						"markers": {{part_electric}}
		    		},
		    		"Qth":
		    		{
						"expr":"materials_Conductor_sigma*(materials_Conductor_U/(2*pi*x))*(materials_Conductor_U/(2*pi*x)):materials_Conductor_sigma:materials_Conductor_U:x",
						"markers": {{part_electric}}
		    		},
		    		"Flaplace":
		    		{
						"expr":"{materials_Conductor_F_laplace_0,materials_Conductor_F_laplace_1}:materials_Conductor_F_laplace_0:materials_Conductor_F_laplace_1",
						"markers": {{part_electric}}
		    		},
		    		"stress":
		    		{
						"expr":"{materials_Conductor_stress_rr,materials_Conductor_stress_rz, materials_Conductor_stress_rz,materials_Conductor_stress_zz}:materials_Conductor_stress_rr:materials_Conductor_stress_rz:materials_Conductor_stress_zz",
						"markers": {{part_thermic}},
						"representation":["element"]
		    		},
		    		"HoopStress":
		    		{
						"expr":"materials_Conductor_stress_thth:materials_Conductor_stress_thth",
						"markers": {{part_thermic}},
						"representation":["element"]
		    		},
		    		"Vonmises":
		    		{
 				                "expr": "sqrt(1/2*((materials_Conductor_stress1-materials_Conductor_stress2)*(materials_Conductor_stress1-materials_Conductor_stress2)+(materials_Conductor_stress2-materials_Conductor_stress3)*(materials_Conductor_stress2-materials_Conductor_stress3)+(materials_Conductor_stress1-materials_Conductor_stress3)*(materials_Conductor_stress1-materials_Conductor_stress3))):materials_Conductor_stress1:materials_Conductor_stress2:materials_Conductor_stress3",
                        			"markers": {{part_thermic}},
						"representation":["element"]
		    		},
		    		"strain":
		    		{
						"expr":"{elastic_grad_u_00,1/2*(elastic_grad_u_01+elastic_grad_u_10), 1/2*(elastic_grad_u_01+elastic_grad_u_10),elastic_grad_u_11}:elastic_grad_u_00:elastic_grad_u_01:elastic_grad_u_10:elastic_grad_u_11",
                    	"markers": {{part_thermic}},
						"representation":["element"]
		    		},
					"HoopStrain":
					{
						"expr": "elastic_u_0/x:x:elastic_u_0",
                        "markers": {{part_thermic}},
						"representation":["element"]
					}

				}
	    	}
		},
		"magnetic":
		{
	    	"Save":
	    	{
				"Fields":
				{
		    		"names":["Atheta"]
				}
	    	},
	    	"Measures": 
	    	{
				"Statistics":
				{
		    		"MagneticEnergy":
		    		{
						"type":"integrate",
						"expr":"-2*pi*magnetic_Atheta*materials_Conductor_sigma*(materials_Conductor_U/2/pi):magnetic_Atheta:materials_Conductor_sigma:materials_Conductor_U:x",
						"markers": {{part_electric}}
					}
                },
				"Points":
				{
					"B0":
					{
                        "coord":"{1e-7,0.}",
                        "fields":"Atheta",
			            "expressions":
                        {
                            "Bz": "magnetic_grad_Atheta_0+magnetic_Atheta/x:magnetic_Atheta:magnetic_grad_Atheta_0:magnetic_grad_Atheta_1:x"
                        }
					}
				}
	    	}
		},
		"heat":
		{
	    	"Save":
	    	{
				"Fields":
				{
		    		"names":["temperature"]
				}
	    	},
	    	"Measures":
	    	{
				"Statistics":
				{
					{{#power_magnet}}
					"Power_{{name}}":
					{
        				"type": "integrate",
        				"expr": "2*pi*materials_Conductor_sigma*(materials_Conductor_U/2/pi)*(materials_Conductor_U/2/pi)/x:materials_Conductor_sigma:materials_Conductor_U:x",
        				"markers": {{magnet_parts}}
					},
					{{/power_magnet}}
		    		"Power":
				    {
        				"type": "integrate",
        				"expr": "2*pi*materials_Conductor_sigma*(materials_Conductor_U/2/pi)*(materials_Conductor_U/2/pi)/x:materials_Conductor_sigma:materials_Conductor_U:x",
        				"markers": {{part_electric}}
    				},
					"Stat_T": 
		    		{
						"type":["min","max","mean"], 
						"field":"temperature" 
		    		}
				}
	    	}
		},
		"elastic":
		{
	    	"Save":
	    	{
				"Fields":
				{
		    		"names":["displacement"]
				}
	    	},
	    	"Measures":
	    	{
				"Statistics":
				{
		    		"Stat_Displ": 
		    		{
						"type":["min","max","mean"], 
						"field":"displacement" 
		    		},
		    		"Stat_Stress": 
		    		{
						"type":["min","max","mean"], 
						"expr":"materials_Conductor_stress_thth:materials_Conductor_stress_thth" 
		    		},
		    		"Stat_VonMises": 
		    		{
						"type":["min","max","mean"], 
						"expr":"sqrt(1/2*((materials_Conductor_stress1-materials_Conductor_stress2)*(materials_Conductor_stress1-materials_Conductor_stress2)+(materials_Conductor_stress2-materials_Conductor_stress3)*(materials_Conductor_stress2-materials_Conductor_stress3)+(materials_Conductor_stress1-materials_Conductor_stress3)*(materials_Conductor_stress1-materials_Conductor_stress3))):materials_Conductor_stress1:materials_Conductor_stress2:materials_Conductor_stress3"
		    		}
				}
	    	}
		}
    }
}
