{
    "Name": "Fully coupled Axi Stationnary model",
    "ShortName":"FullAxiStat",
    "Models":
    {
		"equations":[
	    {
			"name":"heat",
			"unknown":
			{
		    	"basis":"Pch1",
		    	"name":"temperature",
		    	"symbol":"T"
			}
	    },
	    {
			"name":"magnetic",
			"unknown":
			{
			    "basis":"Pch2",
			    "name":"Atheta",
			    "symbol":"Atheta"
			}
	    },
	    {
			"name":"elastic",
			"unknown":
			{
			    "basis":"Pchv1",
			    "name":"displacement",
			    "symbol":"u"
			}
	    }
		]
    },
    "Parameters":
    {
		{{#Parameters}}
		"{{name}}": "{{value}}",
		{{/Parameters}}
		
		"T0":"293.",
		"Tin":"284.15"
    },
    "Materials":
    {
		"Air":
		{
	    	"physics":"magnetic",
	    	"magnetic_c":"x/mu0:x:mu0",
	    	"magnetic_a":"1/mu0/x:x:mu0"
		}
    },
    "BoundaryConditions":
    {
		"magnetic":
		{
		    "Dirichlet":
	    	{
				"magdir":
				{
		    		"markers":["ZAxis","Infty"],
		    		"expr":"0"
				}
			}
	    },
		"heat":
		{
	    	"Robin":
	    	{
				{{#boundary_Therm_Robin}}
				"{{name}}":
				{
					"expr1": "{{expr1}}",
					"expr2": "{{expr2}}"
				},
				{{/boundary_Therm_Robin}}
	    	}
		},
		"elastic":
		{
	    	"Dirichlet":
	    	{
				{{#boundary_Meca_Dir}}
				"{{name}}":
				{
					"expr": "{{value}}"
				},
				{{/boundary_Meca_Dir}}
	    	}
		}
    },
    "InitialConditions":
    {
        "temperature":
        {
            "Expression":
            {
                "myic":
                {
                    "markers": {{part_thermic}},
		    		"expr":"Tinit:Tinit"
                }
            }
        }
    },
    "PostProcess":
    {
		"use-model-name":1,
		"cfpdes":
		{
	    	"Exports":
	    	{
				"fields":["heat.temperature","elastic.displacement"],
				"expr":
				{
		    		"atheta":"magnetic_Atheta:magnetic_Atheta",
		    		"B":
		    		{
						"expr":"{-magnetic_grad_Atheta_1,magnetic_grad_Atheta_0+magnetic_Atheta/x}:magnetic_grad_Atheta_0:magnetic_grad_Atheta_1:magnetic_Atheta:x",
						"representation":["element"]
		    		},
		    		"U":
		    		{
						"expr":"materials_U:materials_U",
						"markers":
						{
			    			"name": ["H%1_1%_Cu%1_2%"],
			    			"index1": {{index_electric}}
						}
		    		},
		    		"Jth":
		    		{
						"expr":"-materials_sigma*materials_U/(2*pi*x):materials_sigma:materials_U:x",
						"markers":
						{
			    			"name": ["H%1_1%_Cu%1_2%"],
			    			"index1": {{index_electric}}
						}
		    		},
		    		"Qth":
		    		{
						"expr":"materials_sigma*(materials_U/(2*pi*x))*(materials_U/(2*pi*x)):materials_sigma:materials_U:x",
						"markers":
						{
			    			"name": ["H%1_1%_Cu%1_2%"],
			    			"index1": {{index_electric}}
						}
		    		},
		    		"Flaplace":
		    		{
						"expr":"{materials_F_laplace_0,materials_F_laplace_1}:materials_F_laplace_0:materials_F_laplace_1",
