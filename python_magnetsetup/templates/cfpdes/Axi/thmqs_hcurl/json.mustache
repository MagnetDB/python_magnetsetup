{
    "Name": "Axi Thermo-Magneto Quasi Static model",
    "ShortName":"AxiThermo-Mqs",
    "Models":
    {
		"cfpdes":{
			"equations":["heat","magnetic"]
		},
		"heat":{
			"common":{
                "setup":{
					"unknown":
					{
						"basis":"Pch1",
						"name":"temperature",
						"symbol":"T"
					}
				}
			},
			"models":[
				
			]
		},
		"magnetic":{
			"common":{
                "setup":{
					"unknown":
					{
						"basis":"Pch1",
						"name":"Atheta",
						"symbol":"Atheta"
					}
				}
			},
			"models":[
				{
					"name":"magnetic_Air",
					"materials":"Air",
					"setup":{
						"coefficients":{
							"c":"x/mu0:x:mu0",
							"a":"1/mu0/x:x:mu0"
						}
					}
				}
			]
		}
    },
    "Parameters":
    {
		{{#Parameters}}
		"{{name}}": "{{value}}",
		{{/Parameters}}

		"T0":"293.",
		"Tin":"284.15"
    },
    "Materials":
    {
		"Air":
		{

		}
    },
    "BoundaryConditions":
    {
		"magnetic":
		{
		    "Dirichlet":
	    	{
				"magdir":
				{
		    		"markers":["ZAxis","Infty"],
		    		"expr":"0"
				}
			}
	    },
		"heat":
		{
	    	"Robin":
	    	{
				{{#boundary_Therm_Robin}}
				"{{name}}": 
				{ 
					"expr1": "{{expr1}}",
					"expr2": "{{expr2}}"
				},
				{{/boundary_Therm_Robin}}
	    	}
		}
    },
    "InitialConditions":
    {
		"heat":{
			"temperature":
			{
				"Expression":
				{
					"myic":
					{
						"markers": {{part_thermic}},
						"expr":"Tinit:Tinit"
					}
				}
			}
		}
    },
    "PostProcess":
    {
		"use-model-name":1,
		"cfpdes":
		{
	    	"Exports":
	    	{
				"fields":["heat.temperature"],
				"expr":
				{
		    		"atheta":"magnetic_Atheta:magnetic_Atheta",
		    		"B":
		    		{
						"expr":"{-magnetic_grad_Atheta_1,magnetic_grad_Atheta_0+magnetic_Atheta/x}:magnetic_Atheta:magnetic_grad_Atheta_0:magnetic_grad_Atheta_1:x",
						"representation":["element"]
		    		},
		    		"U":
		    		{
						"expr":"materials_U:materials_U",
						"markers": {{part_electric}}
		    		},
		    		"Jth":
		    		{
						"expr":"-materials_sigma*(materials_U/(2*pi*x)+magnetic_dAtheta_dt):materials_sigma:materials_U:x:magnetic_dAtheta_dt",
						"markers": {{part_electric}}
		    		},
		    		"Qth":
		    		{
						"expr":"materials_sigma*(materials_U/(2*pi)+magnetic_dAtheta_dt*x)*(materials_U/(2*pi+magnetic_dAtheta_dt*x)):x:materials_sigma:materials_U:magnetic_dAtheta_dt",
						"markers": {{part_electric}}
		    		}
				}
	    	}
		},
		"magnetic":
		{
	    	"Save":
	    	{
				"Fields":
				{
		    		"names":["Atheta"]
				}
	    	},
	    	"Measures": 
	    	{
				"Statistics":
				{
		    		"MagneticEnergy":
		    		{
						"type":"integrate",
						"expr":"-2*pi*magnetic_Atheta*materials_sigma*(materials_U/2/pi+magnetic_dAtheta_dt*x):magnetic_Atheta:magnetic_dAtheta_dt:materials_sigma:materials_U:x",
						"markers": {{part_electric}}
				}
                                },
				"Points":
				{
				"B0":
				{
                                    "coord":"{1e-7,0.}",
                                    "fields":"Atheta",
			            "expressions":
                                    {
                                          "Bz": "magnetic_grad_Atheta_0+magnetic_Atheta/x:magnetic_Atheta:magnetic_grad_Atheta_0:magnetic_grad_Atheta_1:x"
                                    }
				}
			}
	    	}
		},
		"heat":
		{
	    	"Save":
	    	{
				"Fields":
				{
		    		"names":["temperature"]
				}
	    	},
	    	"Measures":
	    	{
				"Statistics":
				{
		    		"MeanT": 
		    		{
						"type":["min","max","mean"], 
						"field":"temperature" 
		    		}
				}
	    	}
		}
    }
}
